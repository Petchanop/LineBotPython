{
    "version": 2,
    "builds": [
        {
            "src": "lineBot/asgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "lineBot/asgi.py"
        }
    ],
    "devCommand": "uvicorn lineBot.asgi:app",
    "env": {
        "APP_MODULE": "lineBot.asgi:app"
    }
}