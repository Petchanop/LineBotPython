{
    "version": 2,
    "devCommand": "uvicorn lineBot.wsgi:app --host 0.0.0.0 --port 3000",
    "builds": [
        {
            "src": "lineBot/lineBot/asgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "lineBot/lineBot/asgi.py"
        }
    ]
}